<h1>Your orders</h1>
<br>
<div class="container">
  <div class="row">
    {{#each purchases}}
    <div class="col-3">
      <img src="{{product.image}}" alt="{{product.title}}">
      <div>
        <p>{{date}}</p>
        <p>{{id}}</p>
        <p class="ml-auto">{{price}}</p>
      </div>
    </div>
  {{/each}}
  </div>
</div>
